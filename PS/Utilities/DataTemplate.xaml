<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:PS.ViewModel"
                    xmlns:view="clr-namespace:PS.View.Pages">
    
    <DataTemplate DataType="{x:Type vm:OrderViewModel}">
        <view:Orders />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HallViewModel}">
        <view:Halls />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PhotographerViewModel}">
        <view:Photographers />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportsViewModel}">
        <view:Reports />
    </DataTemplate>
    
</ResourceDictionary>